        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "workspaceId",
            "in": "path",
            "required": true,
            "type": "string",
            "format": "uuid"
          },
          {
            "name": "itemType",
            "in": "path",
            "required": true,
            "type": "string",
            "enum": ["notebooks", "sparkJobDefinitions", "lakehouses"]
          },
