            "name": "appId",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "attemptId",
            "in": "path",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "jobGroup",
            "in": "query",
            "description": "Only list the timeline of jobs within the given jobGroups. Multiple jobGroups could be specified as: ?jobGroup=1&jobGroup=2",
            "required": false,
            "type": "string"
          },
          {
            "name": "jobLimit",
            "in": "query",
            "description": "The max number of jobs to list per time point",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "executorLimit",
            "in": "query",
            "description": "The max number of executors to list per time point",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "executorJobLimit",
            "in": "query",
            "description": "The max number of executor-job info to list per time point",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "start",
            "in": "query",
            "description": "The lower bound of time points to list",
            "required": false,
            "type": "integer",
            "format": "int64"
          },
          {
            "name": "end",
            "in": "query",
            "description": "The upper bound of time points to list",
            "required": false,
            "type": "integer",
            "format": "int64"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
